{
  "name": "restbooking-app",
  "scripts": {
    "dev": "NODE_OPTIONS='--dns-result-order=ipv4first' next dev -p 3000",
    "build": "NODE_OPTIONS='--dns-result-order=ipv4first' next build 2>&1",
    "bundlesize": "bundlesize",
    "start": "NODE_OPTIONS='--dns-result-order=ipv4first --require ./src/utils/logger.js' node_modules/.bin/next start -H localhost -p 3000",
    "start-inspect": "NODE_OPTIONS='--inspect' next start -p 3000",
    "lint": "eslint . --ext .jsx,.js --quiet",
    "lint:fix": "eslint . --ext .jsx,.js --fix",
    "format": "prettier --write './**/*.{js,jsx,ts,tsx,css,json}' --config ../../common/autoinstallers/rush-husky/.prettierrc",
    "test": "jest --testPathPattern='\\/__tests__\\/(?:unit|integration)/.*\\.(?:test|spec)\\.jsx?$' 2>&1",
    "test-unit": "jest --testPathPattern='\\/__tests__\\/unit\/.*\\.(?:test|spec)\\.jsx?$' 2>&1",
    "test-unit-update-snapshots": "jest -u --testPathPattern='\\/__tests__\\/unit\/.*\\.(?:test|spec)\\.jsx?$'",
    "test-integration": "echo \"Error: no test specified\""
  },
